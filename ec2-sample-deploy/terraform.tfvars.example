# Example terraform.tfvars file
# Copy this file to terraform.tfvars and fill in your actual values

# Name for your EC2 instance (will be used as a tag)
ec2_name = "my-app-server"

# AWS key pair name for SSH access (must exist in your AWS account)
pem_key_name = "my-key-pair"

# AWS region where you want to deploy the instance
aws_region = "us-west-2"

# EC2 instance type (optional, defaults to t3.micro)
ec2_instance_type = "t3.micro"
